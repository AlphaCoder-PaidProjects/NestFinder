                <div class="row">
                    <form action="/owner/owneraddproperties" method="post" enctype="multipart/form-data">
                        
                        <label for="propertyId">Property ID</label>
                        <input type="text" id="propertyId" name="propertyId" placeholder="Enter property ID" required>

                        <label for="title">Title</label>
                        <input type="text" id="title" name="title" placeholder="Enter property title" required>

                        <label for="description">Description</label>
                        <textarea id="description" name="description" placeholder="Enter property description" rows="4"
                            required></textarea>

                        <div class="form-group">
                            <div>
                                <label for="rent">Rent</label>
                                <input type="number" id="rent" name="rent" placeholder="Enter rent amount" required>
                            </div>
                            <div>
                                <label for="deposit">Deposit</label>
                                <input type="number" id="deposit" name="deposit" placeholder="Enter deposit amount"
                                    required>
                            </div>
                        </div>

                        <label for="address">Address</label>
                        <input type="text" id="street" name="street" placeholder="Street" required>
                        <div class="form-group">
                            <div>
                                <input type="text" id="city" name="city" placeholder="City" required>
                            </div>
                            <div>
                                <input type="text" id="state" name="state" placeholder="State" required>
                            </div>
                            <div>
                                <input type="text" id="zipcode" name="zipcode" placeholder="Zip Code" required>
                            </div>
                        </div>

                        <label for="ownerPhone">Owner Phone</label>
                        <input type="tel" id="ownerPhone" name="ownerPhone" placeholder="Enter owner's phone number"
                            required>

                        <label for="ownerEmail">Owner Email</label>
                        <input type="email" id="ownerEmail" name="ownerEmail" placeholder="Enter owner's email address"
                            required>

                        <div class="form-group">
                            <div>
                                <label for="bedrooms">Number of Bedrooms</label>
                                <input type="number" id="bedrooms" name="bedrooms" min="1" required>
                            </div>
                            <div>
                                <label for="bathrooms">Number of Bathrooms</label>
                                <input type="number" id="bathrooms" name="bathrooms" min="1" required>
                            </div>
                        </div>

                        <label for="images">Property Images</label>
                        <input type="file" id="images" name="images" multiple>

                        <label for="mapLocation">Map Location</label>
                        <div class="form-group">
                            <div>
                                <input type="number" step="any" id="latitude" name="latitude" placeholder="Latitude"
                                    required>
                            </div>
                            <div>
                                <input type="number" step="any" id="longitude" name="longitude" placeholder="Longitude"
                                    required>
                            </div>
                        </div>

                        <label for="datePosted">Date Posted</label>
                        <input type="date" id="datePosted" name="datePosted" required>

                        <label for="status">Status</label>
                        <select id="status" name="status" required>
                            <option value="Available">Available</option>
                            <option value="Unavailable">Unavailable</option>
                        </select>



                        <button type="submit">Submit</button>
                    </form>
                </div>
